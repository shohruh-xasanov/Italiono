
  <div class="row">
    <div class="col-12">
      <div style="
          display: flex;
          flex-direction: row;
          justify-content: space-between;
          align-items: center;
        ">
        <h4 class="m-3" style="font-weight: 700">Izlab topilgan malumotlar</h4>
       
      </div>
   
  
      <div class="card m-b-30">
        <div class="card-body">
          <table id="datatable" class="table table-bordered text-center"
            style="border-collapse: collapse; border-spacing: 0; width: 100%">
            <thead>
              <tr>
                <th>Photo</th>
                <th>Nomi</th>
                <th>Parametr</th>
                <th style="width: 15%;">Yaratilgan sana</th>
              </tr>
            </thead>
  
            <tbody>
              <% for(let item of result) {%>
                <tr>
                  <td>
                    <img src="<%= item.poster.min%>" alt="">
                  </td>
                  <%if(item.name.uz){%>
                  <td>
                    <%= item.name.uz %>
                  </td>
                  <%}%>
                  <%if(item.name.ru){%>
                  <td>
                    <%= item.name.ru %>
                  </td>
                  <%}%>
                  <td>
                    <%= moment(item.createdAt).format("DD-MM-YYYY - hh:mm:ss") %>
                  </td>
                  <%if(item.name.uz){%>
                    <td
                      style="
                        display: flex;
                        flex-direction: row;
                        justify-content: space-around;
                        align-items: center;
                      "
                    >
                   
                    <a class="btn btn-warning" href="<%=`/api/product/uz/${item._id}`%>">
                      <i class="fas fa-cog"></i>
                    </a>
                    <form method="POST" class="hidden-r" action="<%=`/api/product/uz/${item._id}?_method=DELETE`%>">
                      <button type="submit" class="btn btn-danger">
                        <i class="fa fa-trash"></i>
                      </button>
                    </form>
                    </td>
                    <% }%>
                    <%if(item.name.ru ){%>
                      <td
                        style="
                          display: flex;
                          flex-direction: row;
                          justify-content: space-around;
                          align-items: center;
                        "
                      >
                     
                      <a class="btn btn-warning" href="<%=`/api/product/ru/${item._id}`%>">
                        <i class="fas fa-cog"></i>
                      </a>
                      <form method="POST" class="hidden-r" action="<%=`/api/product/ru/${item._id}?_method=DELETE`%>">
                        <button type="submit" class="btn btn-danger">
                          <i class="fa fa-trash"></i>
                        </button>
                      </form>
                      </td>
                      <% }%>
                </tr>
                <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <!-- end col -->
  </div>
 
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js"></script>
